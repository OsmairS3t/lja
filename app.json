{
  "expo": {
    "name": "lja",
    "slug": "lja",
    "version": "1.0.0",
    "scheme": "lja",
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Permitir LJA acessar seus dados de Face ID."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "tsconfigPaths": false
    }
  }
}
