{
  "expo": {
    "name": "lja",
    "slug": "lja",
    "version": "1.0.0",
    "scheme": "lja",
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      ["expo-image-picker",
      {
        "photosPermission": "custom photos permission",
        "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",

        "//": "Disables the microphone permission",
        "microphonePermission": false
      }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Permitir LJA acessar seus dados de Face ID."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "tsconfigPaths": false
    }
  }
}
